{
  "name": "yo-inception",
  "version": "0.3.2",
  "description": "Library to run commands on generated code within yeoman generator tests",
  "main": "lib/index.js",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "lint": "eslint lib/*.js",
    "lint:fix": "eslint --fix lib/*.js",
    "jest": "jest __tests__/*.js",
    "test": "npm run lint && npm run jest",
    "coverage": "codecov",
    "prepublish": "nsp check && npm run lint && npm test",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eskalol/yo-inception.git"
  },
  "keywords": [
    "yo",
    "yeoman",
    "mem-fs",
    "mem-fs-editor",
    "yeoman-test"
  ],
  "author": "Eskil Opdahl Nordland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eskalol/yo-inception/issues"
  },
  "homepage": "https://github.com/Eskalol/yo-inception#readme",
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "jest": "^22.0.2",
    "jest-cli": "^22.0.2",
    "nsp": "^3.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "fs-extra": "^8.0.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^4.0.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  }
}
